{
  "name": "emfd",
  "version": "1.0.0",
  "scripts": {
    "build-css": "rm -rf build/css && sass src/sass:build/css --style=compressed --no-source-map --load-path=node_modules",
    "watch-css": "sass src/sass:build/css --style=compressed --watch --load-path=node_modules",
    "start": "npm run watch-css",
    "build": "npm run build-css"
  },
